{
  "name": "beapuls-api-v6",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build --ignore-ts-errors",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "watchsupervisor": "supervisor -W -t dist/bin/server.js",
    "watchtypescript": "node ./node_modules/typescript/bin/tsc -w &",
    "watchserver": "pnpm run build && pnpm run watchtypescript && pnpm run watchsupervisor"
  },
  "imports": {
    "#auth/*": "./app/auth/*.js",
    "#profile/*": "./app/profile/*.js",
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "6.1.3-29",
    "@adonisjs/eslint-config": "^1.2.1",
    "@adonisjs/prettier-config": "^1.2.1",
    "@adonisjs/tsconfig": "^1.2.1",
    "@japa/assert": "^2.1.0",
    "@japa/plugin-adonisjs": "^2.0.3",
    "@japa/runner": "^3.1.1",
    "@swc/core": "^1.3.107",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.14",
    "eslint": "^8.56.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@adonisjs/ally": "5.0.0-7",
    "@adonisjs/auth": "9.0.0-9",
    "@adonisjs/core": "6.1.5-33",
    "@adonisjs/lucid": "19.0.0-6",
    "@adonisjs/session": "7.0.0",
    "@poppinss/utils": "^6.7.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@vinejs/vine": "^1.7.1",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}